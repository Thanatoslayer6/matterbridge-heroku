{
  "name": "Matterbridge-Heroku",
  "description": "Deploy Matterbridge to Heroku",
  "repository": "https://github.com/UG40A/matterbridge-heroku",
  "keywords": ["matterbridge"],
  "success_url": "/",
  "buildpacks": [
    {
      "url": "https://github.com/UG40A/matterbridge-heroku.git#master"
    }
  ],
  "env": {
    "MATTERBRIDGE_VERSION": {
       "description": "Version of matterbridge to download and deploy",
       "value": "1.22.0"
    },
    "WA_NUMBER": {
      "description": "WA number",
      "value": "+xxxxxxxxxxxx"
    },
    "M4_LOGIN": {
      "description": "M4TRIX Login",
      "value": "xxx"
    },
    "M4_PASS": {
      "description": "M4TRIX Password",
      "value": "xxx"
    },
    "M4_HS": {
      "description": "M4TRIX Homeserver",
      "value": "https://xxx.xxx"
    },
    "TG_TOKEN": {
      "description": "Telegram Bot Token",
      "value": "xxx"
    },
    "MSG": {
      "description": "Telegram Message Format",
      "value": "type"
    },
    "WA_CH": {
      "description": "WA Channel",
      "value": "JID@g.us"
    },
    "M4_CH": {
      "description": "M4 Channel",
      "value": "#channel:server"
    }
  }
}
